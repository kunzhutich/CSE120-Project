CREATE TABLE `event` (
  `ID` int DEFAULT NULL,
  `ISPEC` text,
  `WTIDNO` text,
  `PARCEL` text,
  `WATERID` text,
  `LATERAL` text,
  `SIDEGATE` text,
  `NAME1` text,
  `PHONE1` text,
  `RQSTFLO` double DEFAULT NULL,
  `HOURS` double DEFAULT NULL,
  `CROP1` text,
  `IRRIGTYP` text,
  `EVENT_TRANDATE` text,
  `TRANTIME` bigint DEFAULT NULL,
  `EXCESSIVEORDER` text,
  `DELETED` text,
  `SERVAREA` text,
  `FLOWID` text,
  `COMMENT1` text,
  `COMMENT2` text
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
